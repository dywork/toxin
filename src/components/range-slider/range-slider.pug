mixin rangeSlider({ title })
  .range-slider
    .range-slider__view
      h2.range-slider__title= title
      .range-slider__price
        p(class="range-slider__cost js-range-slider-lower-value")
        p(class="range-slider__cost js-range-slider-upper-value")
    div(class="range-slider__slider js-range-slider")
